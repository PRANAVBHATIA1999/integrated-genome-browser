    <target depends="-jdk-init" name="test-genometryImpl">
        <ant antfile="nbproject/ide-file-targets.xml" inheritall="false" target="test-genometryImpl"/>
    </target>
    <target depends="-jdk-init" name="test-das2Server">
        <ant antfile="nbproject/ide-file-targets.xml" inheritall="false" target="test-das2Server"/>
    </target>
    <target depends="-jdk-init" name="test-bookmarks">
        <ant antfile="nbproject/ide-file-targets.xml" inheritall="false" target="test-bookmarks"/>
    </target>
    <target depends="-jdk-init,-jdk-presetdef-nbjpdastart" name="debug-selected-file-in-src">
        <ant antfile="nbproject/ide-file-targets.xml" inheritall="false" target="debug-selected-file-in-src"/>
    </target>
